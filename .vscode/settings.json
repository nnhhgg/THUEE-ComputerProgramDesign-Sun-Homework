{
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ '$fileName' -o ./coin/$fileNameWithoutExt -std=c++17 && echo '----compilation ends.----' && ./coin/$fileNameWithoutExt",
        "c": "cd $dir && gcc '$fileName' -o ./coin/$fileNameWithoutExt -std=c17 && echo '----compilation ends.----' && ./coin/$fileNameWithoutExt"
    },
    "files.associations": {
        "ostream": "cpp"
    },
}